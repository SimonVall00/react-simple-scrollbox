{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-scrollbox",
  "author": "Simon Vall",
  "module": "dist/react-scrollbox.esm.js",
  "size-limit": [
    {
      "path": "dist/react-scrollbox.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-scrollbox.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.6.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "autoprefixer": "^9.8.6",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "rollup-plugin-postcss": "^3.1.8"
  }
}
